{% extends 'base.html.twig' %}

{% block title %}EventVenue index{% endblock %}

{% block body %}
    <h1>EventVenue index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for event_venue in event_venues %}
            <tr>
                <td>{{ event_venue.id }}</td>
                <td>
                    <a href="{{ path('app_event_venue_show', {'id': event_venue.id}) }}">show</a>
                    <a href="{{ path('app_event_venue_edit', {'id': event_venue.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_event_venue_new') }}">Create new</a>
{% endblock %}
